{
    "configurations": [
        {
            "name": "Arduino",
            "includePath": [
                "${workspaceFolder}/**",
                "C:/path/to/Arduino/libraries/**",
                "C:/path/to/Arduino/hardware/arduino/avr/cores/arduino",
                "C:/path/to/Arduino/hardware/arduino/avr/variants/mega"
            ],
            "defines": [
                "ARDUINO=10805",
                "ARDUINO_AVR_MEGA2560",
                "F_CPU=16000000L",
                "USBCON"
            ],
            "compilerPath": "C:/path/to/Arduino/hardware/tools/avr/bin/avr-gcc.exe",
            "cStandard": "c11",
            "cppStandard": "c++11",
            "intelliSenseMode": "gcc-x64"
        }
    ],
    "version": 4
}